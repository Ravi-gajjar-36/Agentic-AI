        "import time\n",
        "def agent1_select_examples(user_question: str, k: int = 2) -> list[dict]:\n",
        "    return retrieve_examples(user_question, k=k)  # from previous FAISS step\n",
        "\n",
        "def answer_question(user_question: str) -> pd.DataFrame:\n",
        "    # 1) Retrieve examples\n",
        "    examples = agent1_select_examples(user_question, k=2)\n",
        "\n",
        "    # 2) Generate SQL with Gemini\n",
        "    schema_str = get_schema_string()\n",
        "    # time.sleep(120)\n",
        "    sql = agent2_generate_sql_gemini(user_question, schema_str, examples)\n",
        "    print(\"Generated SQL:\\n\", sql)\n",
        "\n",
        "    if 'SELECT' in sql:\n",
        "      # 3) Run SQL\n",
        "      df = run_query(sql)\n",
        "      return df\n",
        "    else :\n",
        "      return None"
      ],
