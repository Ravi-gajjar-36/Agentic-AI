        "\n",
        "def embed_text(text: str) -> np.ndarray:\n",
        "    \"\"\"Generate embedding using Gemini.\"\"\"\n",
        "    result = genai.embed_content(model=model, content=text)\n",
        "    return np.array(result['embedding'], dtype=np.float32)"
